<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Shared Calendar Web Application</display-name>
  
  <servlet>
		<servlet-name>cal</servlet-name>
		<servlet-class>org.societies.rdpartyService.enterprise.cal.servlet.CalendarServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>cal</servlet-name>
		<url-pattern>/CalendarServlet/*</url-pattern>
	</servlet-mapping>
</web-app>
