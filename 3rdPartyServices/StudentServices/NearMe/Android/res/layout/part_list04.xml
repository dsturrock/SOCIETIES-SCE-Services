<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:gravity="left"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content">
<LinearLayout 
	android:id="@id/part_list04_layout"
	android:orientation="horizontal"
	android:layout_width="wrap_content"
	android:layout_height="wrap_content" android:gravity="center"
	android:onClick="PartList04"
	android:background="@drawable/part_list01"
	
	>

	
<LinearLayout 
	android:orientation="vertical"
	android:padding="0.0dip" 
	android:layout_height="wrap_content"
	android:layout_marginLeft="2.0dip"
	android:layout_marginTop="5.0dip" android:gravity="center|left" android:layout_width="wrap_content">
	
		<TextView		
			android:id="@id/part_list04_date"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:layout_marginLeft="2.0dip"
			android:text="@string/msn_chat_text"
			android:gravity="left"
			style="@style/PVC.style_02" />
				
		<LinearLayout 
			android:orientation="horizontal"
			android:padding="0.0dip" 
			android:layout_height="wrap_content"
			android:layout_width="320.0dip" android:gravity="center|left">
		<LinearLayout
			android:gravity="center"
			android:adjustViewBounds="true"
			android:background="@drawable/blue_frame"
			android:layout_width="50.0dip" android:layout_height="50.0dip">
			<ImageView 
				android:id="@id/part_list04_image"
				android:layout_height="wrap_content"
				android:layout_width="wrap_content"
				android:maxWidth="40.0dip" 
 				android:minHeight="40.0dip" 
				android:minWidth="40.0dip" 
				android:maxHeight="40.0dip" 
				android:adjustViewBounds="true">
			</ImageView>   		
			</LinearLayout>
			<LinearLayout 
				android:orientation="vertical"
				android:padding="0.0dip" 
				android:gravity="left" android:layout_height="wrap_content" android:layout_width="220.0dip">
				<TextView
					android:id="@id/part_list04_community_name"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="2.0dip"
					android:text="@string/msn_chat_text"
					android:gravity="left"
					style="@style/PVC.style_03" />
				<TextView
					android:id="@id/part_list04_number_of_members"
					android:layout_width="wrap_content"
					android:layout_height="wrap_content"
					android:layout_marginLeft="2.0dip"
					android:text="@string/msn_chat_text"
					android:gravity="left"
					style="@style/PVC.style_03" />
				<TextView
					android:id="@id/part_list04_category"
					android:layout_width="wrap_content"
					android:layout_marginLeft="2.0dip"
					android:text="@string/msn_chat_text"
					android:gravity="left"
					style="@style/PVC.style_03" android:layout_height="20.0dip"/>
				
			</LinearLayout>
			<LinearLayout android:layout_height="40.0dip" android:gravity="center" android:orientation="horizontal" android:layout_width="40.0dip" android:padding="0.0dip">
				<TextView android:layout_height="30.0dip" style="@style/PVC.style_05" android:gravity="center" android:id="@id/part_list04_number_of_new_msm" android:layout_width="30.0dip" android:text="@string/list_member_text_02" android:background="@drawable/yellow_circle"></TextView>
			</LinearLayout>
				
				
		</LinearLayout>			

</LinearLayout>
		
</LinearLayout>
		<LinearLayout
			android:layout_marginTop="4.0dip"
			android:gravity="center"
			android:orientation="horizontal"
			android:padding="0.0dip" 
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:background="@drawable/line">			
		</LinearLayout>	
		
</LinearLayout>