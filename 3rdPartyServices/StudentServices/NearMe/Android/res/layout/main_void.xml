<?xml version="1.0" encoding="utf-8"?>

<LinearLayout android:background="@drawable/bg_default"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:padding="0.0dip"
	android:layout_width="fill_parent" android:layout_height="fill_parent"
	android:gravity="bottom" android:layout_marginLeft="0.0dip"
	android:layout_marginTop="0.0dip">
	<LinearLayout android:id="@id/main_void_topLayout"
		android:gravity="center" android:orientation="vertical"
		android:layout_width="wrap_content" android:layout_height="wrap_content">
	</LinearLayout>

	<ScrollView android:layout_width="fill_parent"
		android:layout_height="fill_parent">
		<LinearLayout android:id="@id/main_void_layout"
			android:gravity="center" android:orientation="vertical"
			android:layout_width="fill_parent" android:layout_height="fill_parent">
		</LinearLayout>
	</ScrollView>


</LinearLayout>